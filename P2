import numpy as np

A = np.array([[1, 2, 3], [4, 5, 6]])
B= np.array([[7, 8], [9, 10], [11, 12]])


result = np.dot(A,B)


print("Matrix 1:")
print(A)
print("\nMatrix 2:")
print(B)
print("\nResult of matrix multiplication:")
print(result)







from datetime import datetime

class DateTimeReader:
    def init(self):
        # Initialize the class, no parameters needed for this example
        pass

    def get_current_date(self):
        # Method to get the current date
        return datetime.now().date()

    def get_current_time(self):
        # Method to get the current time
        return datetime.now().time()

    def get_full_datetime(self):
        # Method to get the full current date and time
        return datetime.now()

# Create an object of the DateTimeReader class
datetime_reader = DateTimeReader()

# Call the methods and display the current date, time, and full datetime
print("Current Date:", datetime_reader.get_current_date())
print("Current Time:", datetime_reader.get_current_time())
print("Current Date and Time:", datetime_reader.get_full_datetime())






def find_duplicates(input_list):
    duplicates = []
    seen = set()  # Using a set to keep track of unique elements

    for item in input_list:
        if item in seen and item not in duplicates:
            duplicates.append(item)  # Append only if it's a duplicate and not already added
        else:
            seen.add(item)

    return duplicates

# Example usage
input_list = [1, 2, 3, 4, 5, 1, 2, 6, 7]
duplicates = find_duplicates(input_list)

print("Input List:", input_list)
print("Duplicate Elements:", duplicates)
